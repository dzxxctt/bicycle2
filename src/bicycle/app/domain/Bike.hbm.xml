<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="bicycle.app.domain.Bike" table="t_bike">
		<id name="bikeId">
			<generator class="uuid"></generator>
		</id>
		<property name="bluetoothId"></property>
		<property name="bikeStatus"></property>
		<property name="longitude"></property>
		<property name="latitude"></property>
		<property name="telphone"></property>
		<property name="address"></property>
		
	</class>
	
</hibernate-mapping>